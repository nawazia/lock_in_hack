# OpenAI API Key (required for embeddings and LLM)
OPENAI_API_KEY=sk-your-openai-key-here

# OpenRouter API Key (required when using openrouter provider)
OPENROUTER_API_KEY=your-openrouter-api-key-here
OPENROUTER_MODEL=openai/gpt-3.5-turbo

# Valyu API Key (required for news search)
VALYU_API_KEY=your-valyu-api-key-here

# Optional: AWS Bedrock credentials (uncomment to use)
TEAM_ID=your-team-id
API_TOKEN=your-api-token
API_ENDPOINT=https://ctwa92wg1b.execute-api.us-east-1.amazonaws.com/prod/invoke

# LLM Provider (openai, openrouter, or bedrock)
LLM_PROVIDER=openai

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
